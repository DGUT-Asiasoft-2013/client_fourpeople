<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_auction"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        android:background="#fde8c6"
        android:orientation="vertical"
        android:weightSum="1">

        <fragment
            android:id="@+id/btn_release"
            class="com.example.fourpeople.campushousekeeper.parttime.fragments_widgets.PartButtonItemFragment"
            android:layout_width="70dp"
            android:layout_height="48dp"
            android:layout_marginTop="1dp"
            tools:layout="@layout/part_fragment_buttonitem"></fragment>

        <fragment
            android:id="@+id/btn_findJobs"
            class="com.example.fourpeople.campushousekeeper.parttime.fragments_widgets.PartButtonItemFragment"
            android:layout_width="71dp"
            android:layout_height="63dp"
            android:layout_marginTop="1dp"
            tools:layout="@layout/part_fragment_buttonitem"></fragment>

        <fragment
            android:id="@+id/btn_findPerson"
            class="com.example.fourpeople.campushousekeeper.parttime.fragments_widgets.PartButtonItemFragment"
            android:layout_width="71dp"
            android:layout_height="72dp"
            android:layout_marginTop="5dp"
            tools:layout="@layout/part_fragment_buttonitem"></fragment>

        <fragment
            android:id="@+id/me"
            class="com.example.fourpeople.campushousekeeper.parttime.fragments_widgets.PartButtonItemFragment"
            android:layout_width="72dp"
            android:layout_height="54dp"
            android:layout_marginTop="1dp"
            tools:layout="@layout/part_fragment_buttonitem"></fragment>

    </LinearLayout>


</FrameLayout>
